{
  "name": "qla-math-platform",
  "version": "3.1.0",
  "description": "QLA Mathematics Platform - Fixed Version with Enhanced Stability",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/run-migrations.js",
    "test": "node scripts/test-platform.js",
    "seed": "node scripts/seed-data.js",
    "build": "echo 'Build phase complete - migrations will run at startup'",
    "fix:sessions": "node scripts/fix-sessions.js",
    "fix:lessons": "node scripts/fix-lessons.js",
    "fix:all": "node scripts/fix-all.js",
    "doctor": "node scripts/fix-all.js",
    "clean": "node -e \"require('pg').Pool({connectionString:process.env.DATABASE_URL}).query('DELETE FROM session WHERE expire < NOW()').then(r=>console.log('Cleaned',r.rowCount,'expired sessions')).catch(console.error)\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "connect-pg-simple": "^9.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dayjs": "^1.11.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "googleapis": "^132.0.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^2.6.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "education",
    "mathematics",
    "learning",
    "interactive",
    "platform",
    "qatar",
    "qla"
  ],
